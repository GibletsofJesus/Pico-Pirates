	for x=0,55 do
		for y=0,26 do
			local c=sget(73+x,36+y)
			local wiggle=y+sin(t()*-.33+(x*.018))*2
			if (c!=0) pset(36+x,4+wiggle,c) pset(36+x,5+wiggle,13)
		end
	end
	--pirates!
	for x=0,98 do
		for y=0,26 do
			local c=sget(30+x,62+y)
			local wiggle=y+sin(t()*-.33+(x*.01))*2
			if (c!=0) pset(15+x,31+wiggle,c) pset(15+x,32+wiggle,13)
		end
	end

	--a game by me
	sspr(32,88,96,16,16,61)
	--twitter tag
	--sspr(31,106,97,11,15,78)
	for x=0,97 do
		for y=0,10 do
			local c=sget(31+x,106+y)
			local start=((t()*66)-y/2)%192
			local stop=start+4
			if (c!=0 and x>start and x<stop) c=7
			if (c!=0) pset(15+x,79+y,c)
		end
	end
	--press x to start
	sspr(30,117,98,11,15,100+sin(t()*.25)*2)--]]
