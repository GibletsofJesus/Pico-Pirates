letter=0

--pirate text
function print_s(_x,_y,_l,c)
	--total_layers=4
	--letters_per_layer=7
	--letter size, 7*7
	local l=_l%7
	local layer=(_l-l)/7
	for x=0,6 do
		for y=0,6 do
			if (band(shl(sget((7*l)+x,16+y)),2^layer)>0) pset(_x+x,y-7+_y,c)
		end
	end
end

function print_l(_x,_y,_l,c)
	local l=_l%7
	local layer=(_l-l)/7
	for x=0,11 do
		for y=0,10 do
			if (band(shl(sget((12*l)+37+x,23+y)),2^layer)>0) pset(_x+x,y-10+_y,c)
		end
	end
end

function print_xl(_x,_y,_l,c)
	local l=_l%3
	local layer=(_l-l)/3
	for x=0,11 do
		for y=0,20 do
			if (band(shl(sget((12*l)+x,23+y)),2^layer)>0) pset(_x+x,y+_y,c)
		end
	end
end
--full lower case alphabet size =7*7		pos=0,16
--upper case alphabet size =12*11 			pos=37,23
--big font size = 12*21									pos=0,23
--big p is 23*26												pos,0,44

function print_str(str,x,y,c)
	str=unhex(str,2)
	local p=x--x position
	print("")
	for s=0,#str-1 do
		if str[s+1]>96 then
			print_s(p,y,str[s+1]-97,c)
			p+=6
		else
			print_l(p,y,str[s+1]-65,c)
			p+=9
			if (str[s+1]<65)p-=6
		end
	end
end

function _draw()
	cls()
	--a->z = 97->122
	--a->z = 65->90

	print_str('412067616d65206d616465206279',24,64,7)
	print_str('43726169672054696e6e6579',26,64+10,7)
	print_str('7b4769626c6574736f664a65737573',16,64+22,12)

	print_str('5072657373205820746f207374617274',16,64+22,12)

end

function unhex(s,n)
 n=n or 2
 local t={}
 for i=1,#s,n do
  add(t,('0x'..sub(s,i,i+n-1))+0)
 end
 return t
end

function __draw()
	cls(0)
	sspr(0,44,12*7,11*4,0,0)
	local total_layers=letter
	local letters_per_layer=7
	for x=0,12*letters_per_layer do
		for y=44,44+10 do
			local total=0
			 for i=0,total_layers do
				 total+=sget(x,y+(11*i))*2^i
			 end
			 pset(x,16+y,total)
			 --sset(x,32+y-1+(7*(total_layers-2)),total)
		end
	end
	local l=letter%letters_per_layer
	local layer=(letter-letter%letters_per_layer)/letters_per_layer
	for x=0,6 do
		for y=48+7*total_layers,54+7*total_layers do
				local col=band(shl(sget(x+(7*l),y)),2^layer)
				if (col>0)col=7
				--pset(x+(7*l),y,15)
				--pset(64+x,y+7,col)
				--pset(64+x-24,y+7-32,col)
		end
	end
	if (btnp(2)) letter=mid(0,letter+1,26)
	if (btnp(3)) letter=mid(0,letter-1,26)
	print("letter: "..letter,0,116,7)

	--[[cls()
	camera(0,0)
	--pico
	--sspr(73,36,55,26,36,4)
