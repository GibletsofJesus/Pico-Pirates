lighthouse={
	bits={},
	init=function()
		local r=12
		for i=0,6,.25 do
			local c=8
			if (flr(i)%2==0) c=7
			r-=0.1
			new_tree_section(-32,-64,i,c,r)
		end
		new_tree_section(-32,-64,6,6,r)
		new_tree_section_h(-32,-64,6.2,5,r)
		r*=.5
		for i=6,6.5,.1 do
			new_tree_section(-32,-64,i,13,r)
		end
		r*=1.1
		for i=6.5,7,.1 do
			r-=0.1
			new_tree_section(-32,-64,i,5,r)
		end
	end,
	update=function()
		for b in all(bits) do
			b.update(b)
		end
	end,
	draw=function()
			for b in all(bits) do
				b.draw(b)
			end
	end
}

