waves={}
function newwave(_x,_y)
	local w={
	x=_x,y=_y,r=2,
	draw=function(w)
		circ(w.x,w.y,w.r,7)
		w.r+=0.2
		if (w.r>10) del(waves,w)
	end
	}
	add(waves,w)
end

