function checkboatpos()
	if (boat.x < -256) then
		boat.x+=512
		currentcellx-=mid(1,#cells,1)
		setcell(currentcellx,currentcelly)
		for w in all(waves) do
			w.x+=512
		end
	end
	if (boat.x > 256) then
		boat.x-=512
		currentcellx+=mid(1,#cells,1)
		setcell(currentcellx,currentcelly)
		for w in all(waves) do
			w.x-=512
		end
	end
	if (boat.y < -256) then
		boat.y+=512
		currentcelly-=mid(1,#cells[currentcellx],1)
		setcell(currentcellx,currentcelly)
		for w in all(waves) do
			w.y+=512
		end
	end
	if (boat.y > 256) then
		boat.y-=512
		currentcelly+=mid(1,#cells,1)
		setcell(currentcellx,currentcelly)
		for w in all(waves) do
			w.y-=512
		end
	end
end

function setcell(x,y)
	createisland(cells[x][y].seed)
end

