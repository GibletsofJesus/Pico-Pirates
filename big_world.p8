	end
	--beach
	for b in all(island_beach) do
		circfill(b.x,b.y,b.rad,15)
	end
	for b in all(island_beach) do
		circfill(b.x+(b.r0*8),b.y+(b.r0*8),b.rad/15,6)
	end
	circfill(0,0,island_size*.8,6)
	fillp(fillps[2])
	circfill(0,0,island_size*1.35,6)
	fillp()
	--draw trees
	for t in all(island_trees) do
	  t.draw(t)
	end
end
