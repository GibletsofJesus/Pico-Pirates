function boat_update(b)
	local speed=2.5
	local s=sin(b.r)
	local c=cos(b.r)
	local sc=(s*s)+(c*c)
	if btn(2) then
		b.x+=s*speed
		b.y-=c*speed
 		b.d+=sc
 		if (flr(b.d)%6==0) newwave(b.x-sin(b.r)*4,b.y+cos(b.r)*4)
	end

	--Moving backwards

	--[[if btn(3) then
	 b.x-=s*speed
	 b.y+=c*speed
	 b.d+=sc
	 if (flr(b.d)%6==0)	newwave(b.x-sin(b.r)*4,b.y+cos(b.r)*4)
 	end]]--

	if(btn(0)) b.r=b.r%1+.025
	if(btn(1)) b.r=b.r%1-.025
	b.x=flr(b.x*2)/2
	b.y=flr(b.y*2)/2

	if (pget(b.x+sin(b.r)*6,b.y-cos(b.r)*6,8)==15) stop()
end

function boat_draw(b)
	if t()%.25==0 then
  	newwave(b.x-sin(b.r)*4,
 	 	b.y+cos(b.r)*4)
 	end
	local s=sin(b.r)
  local c=cos(b.r)
  local _b=s*s+c*c
  local size = 8/2
  local w = sqrt(size^2*2)
  for y=-w,w do
    for x=-w,w do
      local ox=( s*y+c*x)/_b+size
      local oy=(-s*x+c*y)/_b+size
      local col=sget(ox+12,oy+4)
      if col>0 then
        pset(b.x+x,b.y+y,col)
      end
    end
  end
end

